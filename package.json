{
  "name": "@indevweb/moleculer-db-adapter-sequelize",
  "version": "0.0.3",
  "description": "SQL adapter (Postgres, MySQL, SQLite , MSSQL & CockroachDB) for Moleculer DB service",
  "main": "index.js",
  "types": "./index.d.ts",
  "scripts": {
    "dev": "nodemon examples/index.js",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint --ext=.js src test",
    "deps": "npm-check -u",
   "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [
    "microservice",
    "moleculer",
    "orm",
    "database",
    "cockroachdb"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/devalexandre/moleculer-db-adapter-sequelize.git"
  },
  "homepage": "https://github.com/devalexandre/moleculer-db-adapter-sequelize/tree/master/packages/moleculer-db-adapter-sequelize#readme",
  "author": "Alexandre E Souza",
  "license": "MIT",
  "peerDependencies": {
    "moleculer": "^0.12.0 || ^0.13.0 || ^0.14.0",
    "sequelize-cockroachdb": "^6.0.3"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "benchmarkify": "^3.0.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.0.1",
    "jest": "^27.2.5",
    "jest-cli": "^27.2.5",
    "lolex": "^6.0.0",
    "moleculer": "^0.14.17",
    "mysql2": "^2.3.2",
    "nodemon": "^2.0.13",
    "npm-check": "^5.9.2",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize-cockroachdb": "^6.0.3",
    "sqlite3": "^5.0.2",
    "tedious": "^11.0.8"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/services/"
    ]
  },
  "engines": {
    "node": ">= 8.x.x"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "i": "^0.3.7",
    "npm": "^8.1.4"
  }
}
